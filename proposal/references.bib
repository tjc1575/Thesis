	Issn = {1662-5161},
	Journal = {Frontiers in Human Neuroscience},
	Month = sep,
	Pmcid = {PMC4157541},
	Pmid = {25249967},
	Title = {An {EEG}-based mental workload estimator trained on working memory task can work well under simulated multi-attribute task},
