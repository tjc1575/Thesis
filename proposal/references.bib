	Issn = {0169-2607},
	Journal = {Computer Methods and Programs in Biomedicine},
	Keywords = {Adaptive automation, Mental workload, Operator functional state, Paper 7, Recursive feature elimination, Support vector machine},
	Month = jan,
	Number = {1},
	Pages = {101--115},
	Title = {Operator functional state classification using least-square support vector machine based recursive feature elimination technique},
